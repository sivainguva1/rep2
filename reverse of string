package rs;

import java.util.Scanner;

public class rs {

	public static void main(String[] args) {
		String x,r="";
		Scanner sc = new Scanner(System.in);
		System.out.println(" enter the string");
		x= sc.nextLine();
		int l=x.length();
		System.out.println("length="+l);
		for(int i=l-1;i>=0;i--)
		{
		r= r +x.charAt(i);
			
		
	}System.out.println(r);
	}
}


