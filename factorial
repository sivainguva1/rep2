package fact;

import java.util.Scanner;

public class fact {

	public static void main(String[] args) {
		int i,fact=1;
		Scanner sc = new Scanner(System.in);
		System.out.println(" enter the number");
		int n = sc.nextInt();
		for(i=0;i<n;i++) {
		fact=fact*(n-i);		
	
	}

        System.out.println("fact="+fact);
	}
}


